<mvc:View controllerName="com.xtendhr.demo.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">

    <Page id="home" title="{i18n>title}">
        <content>
            <GenericTile id="genericTile" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Products"
                subheader="See our products!" press="onProducts">
                <TileContent id="genericTileContent">
                    <ImageContent id="genericImageContent" src="sap-icon://shipping-status" />
                </TileContent>
            </GenericTile>
        </content>
        <content>
            <GenericTile id="genericTile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="People"
                subheader="See our people!" press="onPeople">
                <TileContent id="genericTileContent2">
                    <ImageContent id="genericImageContent2" src="sap-icon://group" />
                </TileContent>
            </GenericTile>
        </content>
    </Page>
</mvc:View>


